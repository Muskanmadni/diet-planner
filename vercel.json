{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/(.*\\.html)$",
      "dest": "wsgi.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
}