{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "src/diet_planner/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ]
}
