{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/images/(.*)",
      "dest": "/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api.py"
    }
  ]
}